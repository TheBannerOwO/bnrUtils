{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "Build test",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "-Wall",
                "-Wextra",
                "-std=c99",
                "-Iinclude",
                "-L${workspaceFolder}/lib",
                "-lBnrDatastructures",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        },
        {
            "type": "shell",
            "label": "format all C files",
            "command": "bash",
            "args": [
                "${workspaceFolder}/format.sh"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "echo": false,
                "reveal": "never",
                "focus": false,
                "clear": true
            }
        }
    ],
    "version": "2.0.0"
}